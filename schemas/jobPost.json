{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string"
        },
        "salary": {
            "type": "integer",
            "minimum": 0
        },
        "equity": {
            "type": "string"
        },
        "company_handle": {
            "type": "string",
            "minimum": 1,
            "maximum": 25

        }
    },
    "examples": [{
        "title": "chris",
        "salary": "300",
        "equity": "0",
        "company_handle": "crixx"
    }]
}
